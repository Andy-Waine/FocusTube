{
    "manifest_version": 3,
    "name": "FocusTube",
    "description": "Eliminate distractions such as recommended videos, like count, and comments while browsing YouTube.",
    "version": "0.1",
    "icons": {
      "16": "images/logo-white.PNG",
      "32": "images/logo-white.PNG",
      "48": "images/logo-white.PNG",
      "128": "images/logo-white.PNG"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "images/logo-white.PNG",
        "32": "images/logo-white.PNG",
        "48": "images/logo-white.PNG",
        "128": "images/logo-white.PNG"
      }
    },
    "permissions": ["scripting", "activeTab"],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Y",
          "mac": "Command+Y"
        }
      }
    }
  }